{
  "name": "picimo-r3f",
  "version": "0.0.1",
  "description": "Picimo react components based on the fabulous react-three-fiber",
  "module": "build/index.js",
  "types": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel --root-mode upward src/ --extensions \".ts,.tsx,.js,.jsx\" --out-dir ./build/ --source-maps",
    "build:watch": "cross-env BABEL_ENV=production babel --root-mode upward src/ --extensions \".ts,.tsx,.js,.jsx\" --out-dir ./build/ --source-maps --watch",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf build coverage dist",
    "karma": "cross-env NODE_ENV=test karma start --single-run",
    "karma:watch": "cross-env NODE_ENV=test karma start",
    "cb": "run-s clean build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spearwolf/picimo.git"
  },
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/spearwolf/picimo/issues"
  },
  "homepage": "https://github.com/spearwolf/picimo#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.7",
    "assert": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-problems": "^4.0.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-mocha": "^2.0.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "lerna": "^3.20.2",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "picimo": "*",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-three-fiber": "^4.1.1",
    "rimraf": "^3.0.2",
    "three": "^0.116.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "peerDependencies": {
    "picimo": "*",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
